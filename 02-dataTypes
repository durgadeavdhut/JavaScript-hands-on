console.log("====== 1. Number Data Type ========");

var age =20;
console.log("Age is:"+ age)

var ageTypeOf = typeof age;
console.log("Type of age:"+ ageTypeOf);

var weight = 37.50;
console.log("weight is:"+weight);
console.log("Type of weight:"+ typeof weight);

